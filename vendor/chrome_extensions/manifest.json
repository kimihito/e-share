{
  "manifest_version":2,
  "name":"xvhoge",
  "version":"0.0.0",
  "browser_action":"popup.html",
  "permissions":["tabs","http://*/*","https://*/*"],
  "content_scripts":[
    {
      "matches":["http://www.xvideos.jp/video*"],
      "js":["xv.js","script.js"],
      "run_at":"document_end"
    }
  ],
  "background":{"scripts":["xv.js","background.js"]},
  "browser_action":{
    "default_icon":"icon.png"
  }
}
